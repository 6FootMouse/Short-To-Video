{
    "manifest_version": 3,
    "name": "Short to Video",
    "version": "1.1",
    "description": "Open a new tab of the youtube short you're currently on but as a video.",
    "permissions": ["activeTab"],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    }
  }